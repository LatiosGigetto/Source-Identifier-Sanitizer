{
    "manifest_version": 2,
    "name": "Source Identifier Sanitizer",
    "version": "0.1",
    "description": "Removes the Source Identifier parameter from share links",
    "permissions": [
        "https://www.youtube.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*"
            ],
            "js": [
                "siremover.js"
            ]
        }
    ]
}
